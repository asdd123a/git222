package ex02;

public class ex05 {
	
	public static void main(String[] args) {
		// int double String boolean 자주 씀
		
		boolean a = true;
		boolean b = false;
		
		boolean c = false;
		
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		
		System.out.println(20<30);
		System.out.println(20>30);
		
	}

}
